<header>
	<h1>All Contacts</h1>
</header>

	<nav>
		<ul>
			  <% $rolodex.contacts.each do |contact| %>
			    <li><a href="/contacts/<%= contact.id %>">
			    <%= contact.first_name %> <%= contact.last_name %> </a>
					<%= contact.email %> <%= contact.note %>
			    </li>
			  <% end %>			
		</ul>
	</nav>
		 

					

 





